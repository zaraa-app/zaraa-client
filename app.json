{
  "expo": {
    "name": "Zaraa",
    "slug": "zaraa",
    "scheme": "zaraa",
    "version": "1.0.0",
    "newArchEnabled": true,
    "experiments": {
      "tsconfigPaths": true
    },
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#6DBE45"
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "splash": {
        "image": "./assets/images/splash.png",
        "resizeMode": "contain",
        "backgroundColor": "#6DBE45"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#6DBE45"
      }
    },
    "plugins": ["expo-router"]
  }
}
